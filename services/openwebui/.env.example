# API Connection
OPENAI_API_BASE_URL=https://litellm-service.railway.app/v1
OPENAI_API_KEY=your_litellm_master_key

# Authentication Settings
ENABLE_SIGNUP=true
ENABLE_LOGIN_FORM=true
DEFAULT_USER_ROLE=admin

# Document Processing
CONTENT_EXTRACTION_ENGINE=tika
TIKA_SERVER_URL=https://tika-service.railway.app
RAG_EMBEDDING_ENGINE=openai
RAG_EMBEDDING_MODEL=text-embedding-3-small
CHUNK_SIZE=1000
CHUNK_OVERLAP=100

# Web Search
ENABLE_RAG_WEB_SEARCH=true
RAG_WEB_SEARCH_ENGINE=searxng
SEARXNG_QUERY_URL=https://searxng-service.railway.app/search?q=<query>
RAG_WEB_SEARCH_RESULT_COUNT=3

# Code Execution
JUPYTER_TOKEN=your_jupyter_token

# Image Generation
ENABLE_IMAGE_GENERATION=true
IMAGE_GENERATION_ENGINE=openai

# UI Customization
WEBUI_NAME=Unified AI Platform

# Security
WEBUI_AUTH=true
WEBUI_SECRET_KEY=your_webui_secret_key 