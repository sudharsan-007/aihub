# Railway Environment Settings
PORT=3000
LITELLM_PORT=4000

# API Keys - Required
OPENROUTER_API_KEY=your_openrouter_api_key
EDENAI_API_KEY=your_edenai_api_key
LITELLM_MASTER_KEY=your_litellm_master_key

# Authentication
JUPYTER_TOKEN=your_jupyter_token
WEBUI_SECRET_KEY=change-me-in-production

# SearxNG Settings
SEARXNG_BASE_URL=http://localhost:8080/
SEARXNG_INSTANCE_NAME=Unified AI Platform Search
SEARXNG_SECRET_KEY=change-me-in-production

# OpenWebUI Settings
ENABLE_SIGNUP=true
ENABLE_LOGIN_FORM=true
DEFAULT_USER_ROLE=admin
WEBUI_NAME=Unified AI Platform

# Image Generation
ENABLE_IMAGE_GENERATION=true
IMAGE_GENERATION_ENGINE=openai

# Document Processing
RAG_EMBEDDING_ENGINE=openai
RAG_EMBEDDING_MODEL=text-embedding-3-small
CHUNK_SIZE=1000
CHUNK_OVERLAP=100

# Web Search
ENABLE_RAG_WEB_SEARCH=true
RAG_WEB_SEARCH_ENGINE=searxng
RAG_WEB_SEARCH_RESULT_COUNT=3

# Budget Control
BUDGET_DAY=10
BUDGET_WEEK=50
BUDGET_MONTH=200 